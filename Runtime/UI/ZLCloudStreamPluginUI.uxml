                    <ui:Button text="Copy to Clipboard" display-tooltip-when-elided="true" name="copyabout-button" style="width: 139px; flex-direction: row; -unity-text-align: middle-center;" />
                    <ui:Button text="Support" display-tooltip-when-elided="true" name="support-button" style="width: 144px; flex-direction: row; -unity-text-align: middle-center;" />
